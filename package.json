  "bugs": {
    "url": "https://github.com/Planeshifter/node-concept-net/issues"
  },
  "homepage": "https://github.com/Planeshifter/node-concept-net",
  "devDependencies": {
    "coveralls": "^2.11.2",
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-watch": "^0.6.1",
    "istanbul": "^0.3.8",
    "mocha": "^1.21.4"
  },
  "dependencies": {}
